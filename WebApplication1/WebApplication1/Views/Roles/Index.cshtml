@model IEnumerable<WebApplication1.Models.Rol>

@{
    ViewBag.Title = "Rol";
}

<h2>Rol</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreRol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idReunionRol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idSedeRol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idBitacoraRol)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreRol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idReunionRol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idSedeRol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idBitacoraRol)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idRol }) |
            @Html.ActionLink("Details", "Details", new { id=item.idRol }) |
        </td>
    </tr>
}

</table>
